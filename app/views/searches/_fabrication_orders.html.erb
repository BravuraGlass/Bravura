  <h3>Fabrication Orders
     <small>List of existing fabrication_orders</small>
  </h3>
  <div class="container-fluid">
     <div class="row">
        <div class="col-lg-12">
           <div class="panel panel-default">
              <div class="panel-heading">
                 <small></small>
              </div>
              <div class="panel-body">
                 <table class="datatable table table-striped table-hover responsive">
                    <thead>
                      <tr>
                        <th class="sort-alpha">Title</th>
                        <th class="no-sort">Actions</th>
                      </tr>
                    </thead>
                    <tbody>
                       <% @fabrication_orders.each do |fabrication_order| %>
                        <tr>
                          <td><%= fabrication_order.title %></td>
                          <td>
                            <%= link_to 'show',
                                          edit_fabrication_order_path(fabrication_order), :class => 'btn btn-default btn-xs' %>
                            </td>
                        </tr>
                        <% end %>
                    </tbody>
                 </table>
              </div>
           </div>
        </div>
     </div>
  </div>
